  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/property">
    <rdfs:domain>
      <odata4sparql:Operation rdf:about="http://inova8.com/odata4sparql/search/RegexSearch">
        <spin:body>
          <sp:Select>
            <sp:where rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>resource</sp:varName>
                </sp:subject>
                <sp:predicate rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:predicate>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>value</sp:varName>
                </sp:object>
              </rdf:Description>
              <sp:Optional>
                <sp:elements rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>resource</sp:varName>
                    </sp:subject>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>type</sp:varName>
                    </sp:object>
                  </rdf:Description>
                </sp:elements>
              </sp:Optional>
              <sp:Filter>
                <sp:expression>
                  <sp:regex>
                    <sp:arg3>i</sp:arg3>
                    <sp:arg2 rdf:parseType="Resource">
                      <sp:varName>string</sp:varName>
                    </sp:arg2>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName>value</sp:varName>
                    </sp:arg1>
                  </sp:regex>
                </sp:expression>
              </sp:Filter>
            </sp:where>
            <sp:resultVariables rdf:parseType="Collection">
              <rdf:Description>
                <sp:varName>resource</sp:varName>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/resource"/>
                </spl:predicate>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/type"/>
                </spl:predicate>
                <sp:varName>type</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate rdf:resource="http://inova8.com/odata4sparql/search/property"/>
                <sp:varName>property</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <sp:varName>value</sp:varName>
                <spl:predicate>
                  <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/value"/>
                </spl:predicate>
              </rdf:Description>
            </sp:resultVariables>
            <sp:text>select ?resource ?type ?property ?value
where{
  ?resource ?property ?value . 
  OPTIONAL {?resource a ?type }
FILTER( regex(?value,?string,"i"))
}</sp:text>
          </sp:Select>
        </spin:body>
        <spin:constraint>
          <spl:Argument>
            <spl:predicate>
              <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/string"/>
            </spl:predicate>
            <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
          </spl:Argument>
        </spin:constraint>
        <rdfs:label>Regex Search</rdfs:label>
        <skos:prefLabel>Regex Search</skos:prefLabel>
      </odata4sparql:Operation>
    </rdfs:domain>
    <rdfs:domain>
      <odata4sparql:Operation rdf:about="http://inova8.com/odata4sparql/search/LuceneSearch">
        <spin:body>
          <sp:Select>
            <sp:resultVariables rdf:parseType="Collection">
              <rdf:Description>
                <sp:varName>resource</sp:varName>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/resource"/>
                </spl:predicate>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/type"/>
                </spl:predicate>
                <sp:varName>type</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate rdf:resource="http://inova8.com/odata4sparql/search/property"/>
                <sp:varName>property</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate>
                  <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/value"/>
                </spl:predicate>
                <sp:varName>value</sp:varName>
              </rdf:Description>
            </sp:resultVariables>
            <sp:where rdf:parseType="Collection">
              <sp:Optional>
                <sp:elements rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>resource</sp:varName>
                    </sp:subject>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>type</sp:varName>
                    </sp:object>
                  </rdf:Description>
                </sp:elements>
              </sp:Optional>
              <rdf:Description>
                <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#matches"/>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>?0</sp:varName>
                </sp:object>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>resource</sp:varName>
                </sp:subject>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>?0</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>string</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#query"/>
              </rdf:Description>
              <rdf:Description>
                <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#property"/>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:object>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>?0</sp:varName>
                </sp:subject>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>?0</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#score"/>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>score</sp:varName>
                </sp:object>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>?0</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>value</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#snippet"/>
              </rdf:Description>
            </sp:where>
            <sp:text>select ?resource ?type ?property ?value
where{
	  OPTIONAL {?resource a ?type }
	?resource lucenesail:matches   [ 
			lucenesail:query ?string ; 
			lucenesail:property ?property ;
			lucenesail:score ?score ;
			lucenesail:snippet ?value ] . 
}</sp:text>
          </sp:Select>
        </spin:body>
        <spin:constraint>
          <spl:Argument>
            <spl:predicate>
              <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/string"/>
            </spl:predicate>
            <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
          </spl:Argument>
        </spin:constraint>
        <skos:prefLabel>Lucene Search</skos:prefLabel>
        <rdfs:label>Lucene Search</rdfs:label>
      </odata4sparql:Operation>
    </rdfs:domain>
    <rdfs:domain>
      <odata4sparql:Operation rdf:about="http://inova8.com/odata4sparql/search/ElasticSearch">
        <spin:constraint>
          <spl:Argument>
            <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            <spl:predicate>
              <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/string"/>
            </spl:predicate>
          </spl:Argument>
        </spin:constraint>
        <spin:body>
          <sp:Select>
            <sp:resultVariables rdf:parseType="Collection">
              <rdf:Description>
                <sp:varName>resource</sp:varName>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/resource"/>
                </spl:predicate>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/type"/>
                </spl:predicate>
                <sp:varName>type</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <sp:varName>property</sp:varName>
                <spl:predicate rdf:resource="http://inova8.com/odata4sparql/search/property"/>
              </rdf:Description>
              <rdf:Description>
                <sp:varName>value</sp:varName>
                <spl:predicate>
                  <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/value"/>
                </spl:predicate>
              </rdf:Description>
            </sp:resultVariables>
            <sp:text>select ?resource ?type ?property ?value
where{
  ?resource ?property ?value , ?string ^^&lt;http://merck.github.io/Halyard/ns#search&gt; .
  OPTIONAL {?resource a ?type }
}</sp:text> <sp:where rdf:parseType="Collection">
              <rdf:Description>
                <sp:predicate rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:predicate>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>value</sp:varName>
                </sp:object>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>resource</sp:varName>
                </sp:subject>
              </rdf:Description>
              <rdf:Description>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>string</sp:varName>
                </sp:object>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>resource</sp:varName>
                </sp:subject>
                <sp:predicate rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:predicate>
              </rdf:Description>
              <sp:Optional>
                <sp:elements rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>resource</sp:varName>
                    </sp:subject>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>type</sp:varName>
                    </sp:object>
                  </rdf:Description>
                </sp:elements>
              </sp:Optional>
            </sp:where>
          </sp:Select>
        </spin:body>
        <skos:prefLabel>ElasticSearch</skos:prefLabel>
        <rdfs:label>ElasticSearch</rdfs:label>
      </odata4sparql:Operation>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label>property</rdfs:label>
    <skos:prefLabel>property</skos:prefLabel>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/type">
    <rdfs:domain>
      <odata4sparql:Operation rdf:about="http://inova8.com/odata4sparql/search/RegexEntitySearch">
        <spin:constraint>
          <spl:Argument>
            <spl:predicate>
              <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/string"/>
            </spl:predicate>
            <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
          </spl:Argument>
        </spin:constraint>
        <spin:body>
          <sp:Select>
            <sp:resultVariables rdf:parseType="Collection">
              <rdf:Description>
                <sp:varName>entity</sp:varName>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/entity"/>
                </spl:predicate>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate rdf:resource="http://inova8.com/odata4sparql/search/type"/>
                <sp:varName>type</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <sp:varName>formattedValue</sp:varName>
                <spl:predicate>
                  <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/formattedValue"/>
                </spl:predicate>
                <sp:expression>
                  <sp:GroupConcat>
                    <sp:expression>
                      <sp:concat>
                        <sp:arg2 rdf:parseType="Resource">
                          <sp:varName>labelProperty</sp:varName>
                        </sp:arg2>
                        <sp:arg4 rdf:parseType="Resource">
                          <sp:varName>snippet</sp:varName>
                        </sp:arg4>
                        <sp:arg3>&lt;/td&gt;&lt;td&gt;</sp:arg3>
                        <sp:arg1>&lt;tr&gt;&lt;td&gt;</sp:arg1>
                        <sp:arg5>&lt;/td&gt;&lt;/tr&gt;</sp:arg5>
                      </sp:concat>
                    </sp:expression>
                  </sp:GroupConcat>
                </sp:expression>
              </rdf:Description>
            </sp:resultVariables>
            <sp:groupBy rdf:parseType="Collection">
              <rdf:Description>
                <sp:varName>entity</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <sp:varName>type</sp:varName>
              </rdf:Description>
            </sp:groupBy>
            <sp:text>select ?entity ?type (GROUP_CONCAT( concat("&lt;tr&gt;&lt;td&gt;", ?labelProperty, "&lt;/td&gt;&lt;td&gt;" ,?snippet,"&lt;/td&gt;&lt;/tr&gt;") ) as ?formattedValue )
{    
	?entity a ?type  .
	?entity ?property ?snippet . FILTER REGEX(?snippet , ?string, "i")
	?property  rdfs:label ?labelProperty .
}
GROUP BY ?entity ?type</sp:text>
            <sp:where rdf:parseType="Collection">
              <rdf:Description>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>entity</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>type</sp:varName>
                </sp:object>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>entity</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>snippet</sp:varName>
                </sp:object>
                <sp:predicate rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:predicate>
              </rdf:Description>
              <sp:Filter>
                <sp:expression>
                  <sp:regex>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName>snippet</sp:varName>
                    </sp:arg1>
                    <sp:arg3>i</sp:arg3>
                    <sp:arg2>isabel</sp:arg2>
                  </sp:regex>
                </sp:expression>
              </sp:Filter>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>labelProperty</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
              </rdf:Description>
            </sp:where>
          </sp:Select>
        </spin:body>
        <skos:prefLabel>Regex Entity Search</skos:prefLabel>
        <rdfs:label>Regex Entity Search</rdfs:label>
      </odata4sparql:Operation>
    </rdfs:domain>
    <rdfs:domain>
      <odata4sparql:Operation rdf:about="http://inova8.com/odata4sparql/search/LuceneEntitySearch">
        <spin:constraint>
          <spl:Argument>
            <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            <spl:predicate>
              <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/string"/>
            </spl:predicate>
          </spl:Argument>
        </spin:constraint>
        <spin:body>
          <sp:Select>
            <sp:where rdf:parseType="Collection">
              <sp:SubQuery>
                <sp:query>
                  <sp:Select>
                    <sp:resultVariables rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:varName>entity</sp:varName>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:varName>type</sp:varName>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:varName>property</sp:varName>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:varName>snippet</sp:varName>
                      </rdf:Description>
                    </sp:resultVariables>
                    <sp:where rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName>type</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName>entity</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#matches"/>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName>?0</sp:varName>
                        </sp:object>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName>entity</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName>?0</sp:varName>
                        </sp:subject>
                        <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#query"/>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName>string</sp:varName>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName>?0</sp:varName>
                        </sp:subject>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName>property</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#property"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName>score</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#score"/>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName>?0</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName>?0</sp:varName>
                        </sp:subject>
                        <sp:predicate rdf:resource="http://www.openrdf.org/contrib/lucenesail#snippet"/>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName>snippet</sp:varName>
                        </sp:object>
                      </rdf:Description>
                    </sp:where>
                  </sp:Select>
                </sp:query>
              </sp:SubQuery>
              <rdf:Description>
                <sp:object rdf:parseType="Resource">
                  <sp:varName>labelProperty</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName>property</sp:varName>
                </sp:subject>
              </rdf:Description>
            </sp:where>
            <sp:resultVariables rdf:parseType="Collection">
              <rdf:Description>
                <spl:predicate>
                  <owl:ObjectProperty rdf:about="http://inova8.com/odata4sparql/search/entity"/>
                </spl:predicate>
                <sp:varName>entity</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate rdf:resource="http://inova8.com/odata4sparql/search/type"/>
                <sp:varName>type</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <spl:predicate>
                  <owl:DatatypeProperty rdf:about="http://inova8.com/odata4sparql/search/formattedValue"/>
                </spl:predicate>
                <sp:expression>
                  <sp:GroupConcat>
                    <sp:expression>
                      <sp:concat>
                        <sp:arg2 rdf:parseType="Resource">
                          <sp:varName>labelProperty</sp:varName>
                        </sp:arg2>
                        <sp:arg5>&lt;/td&gt;&lt;/tr&gt;</sp:arg5>
                        <sp:arg4 rdf:parseType="Resource">
                          <sp:varName>snippet</sp:varName>
                        </sp:arg4>
                        <sp:arg1>&lt;tr&gt;&lt;td&gt;</sp:arg1>
                        <sp:arg3>&lt;/td&gt;&lt;td&gt;</sp:arg3>
                      </sp:concat>
                    </sp:expression>
                  </sp:GroupConcat>
                </sp:expression>
                <sp:varName>formattedValue</sp:varName>
              </rdf:Description>
            </sp:resultVariables>
            <sp:groupBy rdf:parseType="Collection">
              <rdf:Description>
                <sp:varName>entity</sp:varName>
              </rdf:Description>
              <rdf:Description>
                <sp:varName>type</sp:varName>
              </rdf:Description>
            </sp:groupBy>
            <sp:text>select ?entity ?type (GROUP_CONCAT( concat("&lt;tr&gt;&lt;td&gt;", ?labelProperty, "&lt;/td&gt;&lt;td&gt;" ,?snippet,"&lt;/td&gt;&lt;/tr&gt;") ) as ?formattedValue)
{    
	{
	select ?entity ?type ?property ?snippet
		{
			?entity a ?type .
			?entity lucenesail:matches [
				lucenesail:query ?string ; 
				lucenesail:property ?property ;
				lucenesail:score ?score;
				lucenesail:snippet ?snippet] .
		} 
	} 
	?property  rdfs:label ?labelProperty .
}
GROUP BY ?entity ?type</sp:text>
          </sp:Select>
