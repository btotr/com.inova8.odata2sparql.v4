select ?entityA ?typeA  ?formattedValueA ?p01 ?i10   ?node1  ?p12 ?i21  ?node2 ?p23 ?i32   ?node3 ?p34 ?i43  ?entityB ?typeB ?formattedValueB
{
#  VALUES(?entityA){(&lt;http://northwind.com/Customer-ANTON&gt;)}#(&lt;http://northwind.com/Customer-CONSH&gt;)}
{select (?entity as ?entityA) (?type as ?typeA) (GROUP_CONCAT( concat("&lt;tr&gt;&lt;td&gt;", ?labelProperty, "&lt;/td&gt;&lt;td&gt;" ,?snippet,"&lt;/td&gt;&lt;/tr&gt;") ) as ?formattedValueA)
{    
	{
	select ?entity ?type ?property ?snippet
		{
			?entity a ?type .
			?entity lucenesail:matches [
				lucenesail:query ?stringA ; 
				lucenesail:property ?property ;
				lucenesail:score ?score;
				lucenesail:snippet ?snippet] .
		} 
	} 
	?property  rdfs:label ?labelProperty .
}
GROUP BY ?entity ?type}
#  VALUES(?entityB){(&lt;http://northwind.com/OrderDetail-10507-48&gt;)}#(&lt;http://northwind.com/Product-48&gt;)}#(&lt;http://northwind.com/Order-10507&gt;)(&lt;http://northwind.com/OrderDetail-10507-48&gt;)} 
{select (?entity as ?entityB) (?type as ?typeB) (GROUP_CONCAT( concat("&lt;tr&gt;&lt;td&gt;", ?labelProperty, "&lt;/td&gt;&lt;td&gt;" ,?snippet,"&lt;/td&gt;&lt;/tr&gt;") ) as ?formattedValueB)
{    
	{
	select ?entity ?type ?property ?snippet
		{
			?entity a ?type .
			?entity lucenesail:matches [
				lucenesail:query ?stringB; 
				lucenesail:property ?property ;
				lucenesail:score ?score;
				lucenesail:snippet ?snippet] .
		} 
	} 
	?property  rdfs:label ?labelProperty .
}
GROUP BY ?entity ?type}
	{ #distance = 1
		{?entityA ?p01 ?entityB . }
		UNION
		{?entityB ?i10 ?entityA .}
	}
	UNION
	{	?entityA ?p01 ?node1 .  FILTER(isIRI(?node1) &amp;&amp; ?p01 != rdf:type )
		{ #distance = 2
			{?node1 ?p12 ?entityB .  } 
