							{?node3 ?p34 ?entityB .   } 
							UNION  
							{?entityB ?i43 ?node3 .  } 
						}
					} 
				} 				
			}
			UNION  
			{?node2 ?i21 ?node1 .FILTER(isIRI(?node2) &amp;&amp; ?i21 != rdf:type)  
				{ #distance = 3
					{?node2 ?p23 ?entityB .   } 
						UNION  
