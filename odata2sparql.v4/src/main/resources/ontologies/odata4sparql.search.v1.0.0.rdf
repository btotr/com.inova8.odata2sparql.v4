						{ #distance = 4
							{?node3 ?p34 ?entityB .   } 
							UNION  
							{?entityB ?i43 ?node3 .  } 
						}
					} 
				} 
			}
			UNION  
			{?node2 ?i21 ?node1 . FILTER(isIRI(?node2) &amp;&amp; ?i21 != rdf:type)  
				{ #distance = 3
					{?node2 ?p23 ?entityB .   } 
					UNION  
					{?entityB ?i32 ?node2 .    } 
				}
				UNION
				{ 
					{?node2 ?p23 ?node3. FILTER(isIRI(?node3) &amp;&amp; ?p23 != rdf:type )   
						{ #distance = 4
							{?node3 ?p34 ?entityB .   } 
							UNION  
							{?entityB ?i43 ?node3 .  } 
						}	
					}
					UNION  
					{?node3 ?i32 ?node2 .FILTER(isIRI(?node3) &amp;&amp; ?i32 != rdf:type)  
						{ #distance = 4
							{?node3 ?p34 ?entityB .   } 
							UNION  
							{?entityB ?i43 ?node3 .  } 
						}
					} 
				} 
			} 
		} 
	}
	UNION
	{	?node1 ?i10 ?entityA . FILTER(isIRI(?node1) &amp;&amp; ?i10 != rdf:type) 
		{ #distance = 2
			{?node1 ?p12 ?entityB .    } 
			UNION  
			{?entityB ?i21 ?node1 .  } 
		} 
		UNION
		{ 
			{?node1 ?p12 ?node2. FILTER(isIRI(?node2) &amp;&amp; ?p12 != rdf:type)   
				{ #distance = 3
					{?node2 ?p23 ?entityB .   } 
					UNION  
